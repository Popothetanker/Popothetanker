text = input("insert text here: ")
textA = text.replace('for', '4')
textB = textA.replace('4e', '4')
textC = textB.replace('you', 'u')
textD = textC.replace('are', 'r')
textE = textD.replace('loved', 'simped 4')
textF = textE.replace('wife', 'hoe')
textG = textF.replace('love', 'luv')
textH = textG.replace('good', 'gud')
textI = textH.replace('delicious', 'bussin bussin')
textJ = textI.replace('tasty', 'bussin bussin')
print(textJ)
